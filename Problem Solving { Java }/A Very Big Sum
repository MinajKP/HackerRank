import java.util.Scanner;

public class bigSum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
        int input = scan.nextInt();
        long sum = 0;
        
        for(int i = 0; i < input; i++){
            sum += scan.nextLong();
        }
        
        System.out.print(sum); 

}
}
