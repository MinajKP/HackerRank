import java.util.Scanner;

public class conditional {

	public static void main(String[] args) {
		
		Scanner scanner= new Scanner (System.in);
		int n = scanner.nextInt();
		
		if(n%2==1) {
			System.out.println("Weird");
		}
		else if(n <= 5 & n >= 2) {
			System.out.println("Not Weird");
		}
		else if (n >= 6 && n <= 21) {
			System.out.println("Weird");
		}
		else if(n>20) {
			System.out.println("Not Weird");
		}
	}
}
