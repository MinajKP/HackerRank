using System;

    class Program
    {
        static void Main(string[] args)
        {
            var sum = 0L;
            var first = int.Parse(Console.ReadLine());
            var second = Console.ReadLine().Split(' ');
            var array = Array.ConvertAll(second, long.Parse);
            for(int i=0; i < first; i++)
            
                sum += array[i];
            
            Console.WriteLine(sum);
        }
    }
