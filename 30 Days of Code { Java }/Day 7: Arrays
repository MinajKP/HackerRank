public class Solution {

    public static void main(String[] args) {
         Scanner scan = new Scanner(System.in);
        int size = scan.nextInt(); 
        int[] value = new int[size];
        
        for(int i=0; i<size; i++) {
            value[i] = scan.nextInt();        
        }
        
        for (int i = 0; i < size; i++) {
            int rev = size-i-1;
            System.out.print(value[rev]);
            System.out.print(" ");
    }
}
}
